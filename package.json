{
  "name": "node-react-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.17.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "build": "react-scripts build",
    "start": "node index.js",
    "dev": "npm run build && npm start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "index.js",
      "src",
      "routes"
    ],
    "ignore": [
      "node_modules",
      "public"
    ],
    "exec": "npm run dev"
  }
}
